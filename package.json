{
  "name": "graphql-ts-server-boilerplate",
  "version": "0.0.1",
  "description": "Awesome project developed with TypeORM.",
  "devDependencies": {
    "@types/bcryptjs": "^2.4.1",
    "@types/ioredis": "^3.2.8",
    "@types/jest": "^23.0.0",
    "@types/node": "^10.3.1",
    "@types/node-fetch": "^2.1.1",
    "@types/uuid": "^3.4.3",
    "@types/yup": "^0.24.6",
    "cross-env": "^5.1.6",
    "gql2ts": "^1.8.0",
    "jest": "^23.1.0",
    "nodemon": "^1.17.5",
    "ts-jest": "^22.4.6",
    "ts-node": "6.1.0",
    "tslint": "^5.10.0",
    "tslint-config-prettier": "^1.13.0",
    "typescript": "2.9.1"
  },
  "dependencies": {
    "@gql2ts/from-schema": "^1.8.0",
    "bcryptjs": "^2.4.3",
    "graphql-import": "^0.6.0",
    "graphql-request": "^1.6.0",
    "graphql-tools": "^3.0.2",
    "graphql-yoga": "^1.14.7",
    "ioredis": "^3.2.2",
    "node-fetch": "^2.1.2",
    "pg": "^7.4.3",
    "reflect-metadata": "^0.1.12",
    "typeorm": "0.2.7",
    "uuid": "^3.2.1",
    "yup": "^0.25.1"
  },
  "scripts": {
    "start": "cross-env NODE_ENV=development nodemon --exec ts-node src/index.ts",
    "test": "cross-env NODE_ENV=test jest --runInBand --forceExit",
    "gen-schema-types": "ts-node src/scripts/createTypes.ts"
  },
  "jest": {
    "globalSetup": "./src/testSetup/callSetup.js",
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  }
}
